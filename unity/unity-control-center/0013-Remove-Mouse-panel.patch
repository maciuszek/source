-	if (gtk_toggle_button_get_active (GTK_TOGGLE_BUTTON (WID ("two_finger_scroll_toggle"))))
-		method = GSD_TOUCHPAD_SCROLL_METHOD_TWO_FINGER_SCROLLING;
-	else

-      return 1;
-    }
-
-  window = gnome_mouse_test_init (builder);
-  gtk_widget_show_all (window);
-
-  g_signal_connect (G_OBJECT (window), "delete-event",
-  G_CALLBACK (delete_event_cb), window);
-
-  gtk_main ();
-
-  return 0;
-}

-  gtk_main_quit ();
-
-  return FALSE;
-}
-
-int main (int argc, char **argv)
-{
-  GtkBuilder *builder;
-  GtkWidget *window;
-  GError *error = NULL;
-
-  gtk_init (&argc, &argv);
-
-  builder = gtk_builder_new ();
-
-  gtk_builder_add_from_file (builder, "gnome-mouse-test.ui", &error);

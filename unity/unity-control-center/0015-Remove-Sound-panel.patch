-                if (control->priv->reconnect_id == 0)
-                        control->priv->reconnect_id = g_timeout_add_seconds (RECONNECT_DELAY, idle_reconnect, control);
-                break;
-
-        case PA_CONTEXT_TERMINATED:
-        default:
-                /* FIXME: */
-                break;
-        }
